views:
  - type: cards
    name: Notes
    filters:
      and:
        - file.inFolder("INFO/Patterns")
    order:
      - file.name
      - file.links
    cardSize: 420
    image: note.cover
    imageAspectRatio: 0.55
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("INFO/Patterns")
    order:
      - file.name
      - cover
